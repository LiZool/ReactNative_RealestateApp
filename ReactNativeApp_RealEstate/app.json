{
  "expo": {
    "name": "ReactNativeApp_RealEstate",
    "slug": "ReactNativeApp_RealEstate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/owl.png",
    "scheme": "real estate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/pngwing.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"  
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pngwing.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/owl.png",
          /*"imageWidth": 200, */
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage_legacy": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
